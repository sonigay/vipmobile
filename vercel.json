{
  "buildCommand": "npm run build:ultra-fast",
  "outputDirectory": "build",
  "installCommand": "npm ci --prefer-offline --no-audit --no-fund --silent",
  "framework": "create-react-app",
  "env": {
    "REACT_APP_API_URL": "https://vipmobile-backend.cloudtype.app",
    "REACT_APP_LOGGING_ENABLED": "true",
    "REACT_APP_PUSH_NOTIFICATIONS_ENABLED": "true",
    "GENERATE_SOURCEMAP": "false",
    "INLINE_RUNTIME_CHUNK": "false",
    "CI": "false",
    "NODE_ENV": "production",
    "SKIP_PREFLIGHT_CHECK": "true",
    "FAST_REFRESH": "false",
    "TSC_COMPILE_ON_ERROR": "false",
    "ESLINT_NO_DEV_ERRORS": "true",
    "DISABLE_ESLINT_PLUGIN": "true"
  }
}
